      "~/i18n/*": [
        "app/i18n/*"
      ],
      "~/models": [
        "app/models"
      ],
      "~/models/*": [
        "app/models/*"
      ],
      "~/services": [
        "app/services"
      ],
      "~/services/*": [
        "app/services/*"
