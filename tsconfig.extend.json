        "app"
      ],
      "@portal/*": [
        "app/*"
      ],
      "~/assets": [
        "app/assets"
      ],
      "~/assets/*": [
        "app/assets/*"
      ],
      "~/config": [
        "app/config"
      ],
      "~/config/*": [
        "app/config/*"
      ],
      "~/i18n": [
        "app/i18n"
